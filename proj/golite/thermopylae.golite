package main;

import "fmt";

func fact (x int) int  {
	if(x <= 1) {
		return 1;
	} else {
		return (x * fact(x-1));
	}
}

func main () {
	var stop bool;
	var factor, toStop, temp int;

	stop = false;
	factor = 0;
	
	for(!stop) {
		fmt.Scan(&factor);
        temp = fact(factor);
        fmt.Println(temp); 
        fmt.Scan(&toStop);
	
		if(toStop == 0) {
			stop = true;
		}
	}
}
